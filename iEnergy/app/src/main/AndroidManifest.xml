<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dominik.bakalarka">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        >
        <activity android:name="com.example.dominik.bakalarka.LoginRegister.Login"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name="com.example.dominik.bakalarka.Function.Info"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name="com.example.dominik.bakalarka.LoginRegister.UpdateData"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name="com.example.dominik.bakalarka.LoginRegister.Register"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name="com.example.dominik.bakalarka.Main.MainActivity"
            android:theme="@style/OtherTheme">

        </activity>
        <activity android:name="com.example.dominik.bakalarka.Intro.DefaultIntro"
            android:theme="@style/OtherTheme">
        </activity>
        <activity android:name="com.example.dominik.bakalarka.Function.Faktura"
            android:theme="@style/CustomTheme">
        </activity>
        <activity android:name="com.example.dominik.bakalarka.Graph.PieChartActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name="com.example.dominik.bakalarka.Saving.Saving"
            android:theme="@style/OtherTheme">
        </activity>
        <activity android:name="com.example.dominik.bakalarka.Saving.SavingOptions"
            android:theme="@style/OtherTheme">
        </activity>

        <activity android:name=".Intro.Welcome"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>